<h3>Лекторы</h3>

<table class="table table-striped">
  <thead>
    <tr>
      <th>First name</th>
      <th>Father name</th>
      <th>Last name</th>
      <th>Bio</th>
      <th>Photo url</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @lectors.each do |lector| %>
      <tr>
        <td><%= lector.first_name %></td>
        <td><%= lector.father_name %></td>
        <td><%= lector.last_name %></td>
        <td><%= lector.bio %></td>
        <td><%= lector.photo_url %></td>
        <td><%= link_to 'Show', lector %></td>
        <td><%= link_to 'Edit', edit_lector_path(lector) %></td>
        <td><%= link_to 'Destroy', lector, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Lector', new_lector_path %>