<h1>Listing info_settings</h1>

<table>
  <thead>
    <tr>
      <th>Category</th>
      <th>Url</th>
      <th>Title rus</th>
      <th>Title eng</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @info_settings.each do |info_setting| %>
      <tr>
        <td><%= info_setting.category %></td>
        <td><%= info_setting.url %></td>
        <td><%= info_setting.title_rus %></td>
        <td><%= info_setting.title_eng %></td>
        <td><%= link_to 'Show', info_setting %></td>
        <td><%= link_to 'Edit', edit_info_setting_path(info_setting) %></td>
        <td><%= link_to 'Destroy', info_setting, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Info setting', new_info_setting_path %>
