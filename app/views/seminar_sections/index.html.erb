<h1>Listing seminar_sections</h1>

<table>
  <thead>
    <tr>
      <th>Section</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @seminar_sections.each do |seminar_section| %>
      <tr>
        <td><%= seminar_section.section %></td>
        <td><%= link_to 'Show', seminar_section %></td>
        <td><%= link_to 'Edit', edit_seminar_section_path(seminar_section) %></td>
        <td><%= link_to 'Destroy', seminar_section, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Seminar section', new_seminar_section_path %>
